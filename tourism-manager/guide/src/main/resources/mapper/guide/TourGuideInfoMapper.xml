<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.guide.mapper.TourGuideInfoMapper">
    
    <resultMap type="TourGuideInfo" id="TourGuideInfoResult">
        <result property="id"    column="id"    />
        <result property="name"    column="name"    />
        <result property="phone"    column="phone"    />
        <result property="identityId"    column="identity_id"    />
        <result property="agencyId"    column="agency_id"    />
        <result property="qualify"    column="qualify"    />
        <result property="licenseNumber"    column="license_number"    />
        <result property="licenseEndTime"    column="license_end_time"    />
        <result property="licenseStartTime"    column="license_start_time"    />
        <result property="createTime"    column="create_time"    />
        <result property="createUser"    column="create_user"    />
        <result property="modifyTime"    column="modify_time"    />
        <result property="modifyUser"    column="modify_user"    />
    </resultMap>

    <sql id="selectTourGuideInfoVo">
        select id, name, phone, identity_id, agency_id, qualify, license_number, license_end_time, license_start_time, create_time, create_user, modify_time, modify_user from tour_guide_info
    </sql>

    <select id="selectTourGuideInfoList" parameterType="TourGuideInfo" resultMap="TourGuideInfoResult">
        <include refid="selectTourGuideInfoVo"/>
        <where>  
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="phone != null  and phone != ''"> and phone = #{phone}</if>
            <if test="identityId != null  and identityId != ''"> and identity_id = #{identityId}</if>
            <if test="agencyId != null "> and agency_id like concat('%', #{agencyId}, '%')</if>
            <if test="licenseNumber != null  and licenseNumber != ''"> and license_number = #{licenseNumber}</if>
            <if test="licenseEndTime != null "> and license_end_time like concat('%', #{licenseEndTime}, '%')</if>
            <if test="licenseStartTime != null "> and license_start_time like concat('%', #{licenseStartTime}, '%')</if>
        </where>
    </select>
    
    <select id="selectTourGuideInfoById" parameterType="Long" resultMap="TourGuideInfoResult">
        <include refid="selectTourGuideInfoVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertTourGuideInfo" parameterType="TourGuideInfo" useGeneratedKeys="true" keyProperty="id">
        insert into tour_guide_info
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="name != null">name,</if>
            <if test="phone != null">phone,</if>
            <if test="identityId != null">identity_id,</if>
            <if test="agencyId != null">agency_id,</if>
            <if test="qualify != null">qualify,</if>
            <if test="licenseNumber != null">license_number,</if>
            <if test="licenseEndTime != null">license_end_time,</if>
            <if test="licenseStartTime != null">license_start_time,</if>
            <if test="createTime != null">create_time,</if>
            <if test="createUser != null">create_user,</if>
            <if test="modifyTime != null">modify_time,</if>
            <if test="modifyUser != null">modify_user,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="name != null">#{name},</if>
            <if test="phone != null">#{phone},</if>
            <if test="identityId != null">#{identityId},</if>
            <if test="agencyId != null">#{agencyId},</if>
            <if test="qualify != null">#{qualify},</if>
            <if test="licenseNumber != null">#{licenseNumber},</if>
            <if test="licenseEndTime != null">#{licenseEndTime},</if>
            <if test="licenseStartTime != null">#{licenseStartTime},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="createUser != null">#{createUser},</if>
            <if test="modifyTime != null">#{modifyTime},</if>
            <if test="modifyUser != null">#{modifyUser},</if>
         </trim>
    </insert>

    <update id="updateTourGuideInfo" parameterType="TourGuideInfo">
        update tour_guide_info
        <trim prefix="SET" suffixOverrides=",">
            <if test="name != null">name = #{name},</if>
            <if test="phone != null">phone = #{phone},</if>
            <if test="identityId != null">identity_id = #{identityId},</if>
            <if test="agencyId != null">agency_id = #{agencyId},</if>
            <if test="qualify != null">qualify = #{qualify},</if>
            <if test="licenseNumber != null">license_number = #{licenseNumber},</if>
            <if test="licenseEndTime != null">license_end_time = #{licenseEndTime},</if>
            <if test="licenseStartTime != null">license_start_time = #{licenseStartTime},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="createUser != null">create_user = #{createUser},</if>
            <if test="modifyTime != null">modify_time = #{modifyTime},</if>
            <if test="modifyUser != null">modify_user = #{modifyUser},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTourGuideInfoById" parameterType="Long">
        delete from tour_guide_info where id = #{id}
    </delete>

    <delete id="deleteTourGuideInfoByIds" parameterType="String">
        delete from tour_guide_info where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>